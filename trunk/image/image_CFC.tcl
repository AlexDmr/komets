inherit Image_CFC CommonFC

#___________________________________________________________________________________________________________________________________________
method Image_CFC constructor {} {
 set this(img_file_name) ""
}

#___________________________________________________________________________________________________________________________________________
method Image_CFC load_img          {f} {set this(img_file_name) $f}

#___________________________________________________________________________________________________________________________________________
method Image_CFC get_img_file_name { } {return $this(img_file_name)}

#___________________________________________________________________________________________________________________________________________
#___________________________________________________________________________________________________________________________________________
#___________________________________________________________________________________________________________________________________________
proc P_L_methodes_get_Image {} {return [list {get_img_file_name {}}]}
proc P_L_methodes_set_Image {} {return [list {load_img {f}}]}

                                      